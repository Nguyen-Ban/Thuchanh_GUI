   <children>
      <Button fx:id="open" layoutX="156.0" layoutY="251.0" mnemonicParsing="false" onAction="#chooseImage" text="open" />
      <Button fx:id="exit" layoutX="386.0" layoutY="251.0" mnemonicParsing="false" text="exit" />
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="200.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true" />
